<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="amhiggin.scss.tcd.ie.carmeleon.Newsfeed"
    android:visibility="visible">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/centraOffer"
        android:maxHeight="2000dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:minWidth="200dp"
        android:minHeight="200dp"
        android:layout_above="@+id/insomniaOffer"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/insomniaOffer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/centraOffer"
        android:layout_above="@+id/topazOffer"
        android:minWidth="200dp"
        android:minHeight="200dp"
        android:nestedScrollingEnabled="false" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/topazOffer"
        android:layout_below="@id/insomniaOffer"
        android:minHeight="200dp"
        android:minWidth="200dp"
        android:longClickable="true" />



</RelativeLayout>
